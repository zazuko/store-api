    "coveralls": "^3.0.11",
    "deep-equal": "^2.0.5",
    "eslint": "^7.19.0",
    "get-stream": "^6.0.1",
    "jsdoc": "^3.6.6",
    "mocha": "^8.2.1",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "graphdb": "^2.0.0",
    "is-stream": "^2.0.0",
    "node-fetch": "^2.6.1",
